<resources>

    <!-- Base application theme. -->
    <style name="QiShuiAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!--导航栏颜色-->
        <item name="colorPrimary">@color/colorMain</item>
        <!--状态栏颜色 5.0 之后有效-->
        <item name="colorPrimaryDark">@color/colorMain</item>
        <!--控件选中后颜色-->
        <item name="colorAccent">@color/colorMain</item>
    </style>


    <style name="SplashTheme" parent="QiShuiAppTheme">
        <item name="android:windowBackground">@color/colorMain</item>
    </style>
    
</resources>
